    "@typescript-eslint/parser": "^5.26.0",
    "@viclafouch/eslint-config-viclafouch": "^3.6.2",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
